{
  "name": "vite-vue3-tpl",
  "version": "0.0.0",
  "license": "ISC",
  "docker_package": "const/page-blog:front-v1",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:mock": "vite --mode mock",
    "dev:force": "vite --mode dev --force",
    "build": "vue-tsc --noEmit && vite build --mode dev",
    "build:prod": "vue-tsc --noEmit && vite build --mode prod",
    "serve": "vite preview --mode prod",
    "prepare": "husky install",
    "test": "jest",
    "cz": "git add . && git cz",
    "docker:image": "docker build -t %npm_package_docker_package% .",
    "docker:push": "docker push %npm_package_docker_package%",
    "docker": "pnpm docker:image && pnpm docker:push",
    "release": "pnpm buildProd && pnpm docker"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "axios-jsonp": "^1.0.4",
    "axios-retry": "^3.3.1",
    "mockjs": "^1.1.0",
    "pinia": "2.0.16",
    "pinia-plugin-persist": "^1.0.0",
    "postcss": "^8.4.14",
    "qs": "^6.11.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@constq/eslint-config-qzr": "^0.0.5",
    "@constq/qzr-utils": "^1.2.4",
    "@types/jest": "^28.1.6",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^18.0.6",
    "@types/qs": "^6.9.7",
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/test-utils": "^2.0.2",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.1",
    "eslint": "^8.20.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "postcss-px-to-viewport": "^1.1.1",
    "rollup-plugin-visualizer": "^5.7.1",
    "sass": "^1.53.0",
    "stylus": "^0.58.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.9.4",
    "unplugin-vue-components": "^0.21.1",
    "vite": "^3.0.2",
    "vite-plugin-compression": "^0.5.1",
    "vue-global-api": "^0.4.1",
    "vue-jest": "^3.0.7",
    "vue-tsc": "^0.38.9"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }
}
